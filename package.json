{
  "name": "tyranid",
  "version": "0.0.165",
  "description": "Metadata Library to faciliate working generically with data (MongoDB, ODM)",
  "main": "dist/tyranid.js",
  "scripts": {
    "test": "npm run mocha",
    "babel": "./node_modules/.bin/babel ./dist-es6 --out-dir ./dist-node4",
    "build": "rm -rf ./dist-es6/ ./dist-node4/ && npm run typings && npm run tscompile && npm run babel && cp ./package.json ./dist-node4/",
    "copy-typings": "cp -r ./typings ./dist-es6 && cp -r ./typings ./dist-node4",
    "docs": "node -e \"require('./doc/doc').default();\"",
    "mocha": "./node_modules/.bin/mocha --compilers js:babel-register ./dist-node4/test/test.js",
    "pretest": "npm run build",
    "prepublish": "npm test",
    "tscompile": "./node_modules/typescript/bin/tsc",
    "typings": "rm -rf ./typings/ && ./node_modules/.bin/typings install",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js ./test/**/*.js",
    "watch-babel": "./node_modules/.bin/babel --watch ./dist/es6 --out-dir ./dist/node4",
    "watch-tsc": "./node_modules/typescript/bin/tsc -w -d -p ./",
    "watch": "./node_modules/.bin/concurrently --kill-others 'npm run watch-babel' 'npm run watch-tsc'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyranid-org/tyranid"
  },
  "engine": {
    "node": ">=4.3.0"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^1.1.1",
    "continuation-local-storage": "^3.1.6",
    "faker": "^3.1.0",
    "hooker": "^0.2.3",
    "lodash": "^3.9.3",
    "moment": "^2.11.2",
    "mongodb": "^2.1.4",
    "on-finished": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-preset-stage-0": "6.5.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "1.1.1",
    "gulp-mocha": "^2.1.1",
    "gulp-sourcemaps": "^1.5.2",
    "mocha": "^2.2.5",
    "typescript": "^1.9.0-dev.20160417",
    "typings": "^0.7.12",
    "yargs": "^3.12.0"
  },
  "babel": {
    "sourceMaps": "both",
    "compact": false,
    "presets": [
      "stage-0",
      "es2015-node4"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
