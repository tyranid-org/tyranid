{
  "name": "tyranid",
  "version": "0.4.52",
  "description": "Metadata Library to faciliate working generically with data (MongoDB, ODM)",
  "main": "dist/src/tyranid.js",
  "typings": "src/tyranid.d.ts",
  "scripts": {
    "test": "npm run mocha",
    "test-travis": "npm run pretest && npm run mocha-coverage",
    "pretest": "npm run lint && npm run tsc",
    "prepublish": "npm run test",
    "watch": "rm -rf ./dist/ && tsc -w",
    "tsc": "rm -rf ./dist/ && tsc",
    "lint": "tslint './{test,src}/**/*.{ts,js}' --format stylish --fix",
    "mocha": "mocha \"./dist/test/test.js\" --read false",
    "mocha-coverage": "nyc mocha \"./dist/test/test.js\" --read false",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "ts-node ./doc/doc.js",
    "server": "node ./test/serverBoot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyranid-org/tyranid"
  },
  "engine": {
    "node": ">=8.1.4"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^2.1.0",
    "continuation-local-storage": "3.1.6",
    "faker": "3.1.0",
    "glob": "7.0.5",
    "json-stable-stringify": "1.0.1",
    "lodash": "^3.10.1",
    "moment": "2.11.2",
    "on-finished": "2.3.0",
    "socket.io": "2.0.3",
    "uglify-js": "2.8.16",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "typescript": "^2.5.3",
    "mongodb": "^2.2.33"
  },
  "devDependencies": {
    "@types/chai": "3.5.0",
    "@types/express": "4.0.36",
    "@types/lodash": "3.10.1",
    "@types/mocha": "2.2.43",
    "@types/mongodb": "2.2.7",
    "body-parser": "1.17.2",
    "chai": "3.5.0",
    "chai-as-promised": "5.1.0",
    "codecov": "1.0.1",
    "eslint": "3.9.0",
    "express": "4.15.2",
    "jquery": "3.1.1",
    "jsdom": "9.8.3",
    "mocha": "2.5.3",
    "mongodb": "2.2.33",
    "node-fetch": "1.7.1",
    "nyc": "8.3.0",
    "ts-node": "3.2.0",
    "typescript": "^2.5.3",
    "tslint": "5.5.0",
    "yargs": "3.12.0"
  }
}
