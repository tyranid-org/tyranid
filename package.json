{
  "name": "tyranid",
  "version": "0.2.1",
  "description": "Metadata Library to faciliate working generically with data (MongoDB, ODM)",
  "main": "dist/tyranid.js",
  "jsnext:main": "src/tyranid.js",
  "typings": "src/tyranid.d.ts",
  "scripts": {
    "test": "npm run mocha && npm run tsc",
    "test-travis": "npm run pretest && npm run mocha-coverage && npm run tsc",
    "pretest": "npm run babel",
    "prepublish": "npm run test",
    "watch": "babel src --out-dir dist -q -w",
    "babel": "babel src --out-dir dist -q",
    "eslint": "eslint ./src ./test --ignore-pattern test/test-typings.js",
    "mocha": "mocha \"./test/test.js\" --read false --require \"babel-core/register\"",
    "mocha-coverage": "nyc --require babel-core/register mocha \"./test/test.js\" --read false",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "tsc": "tsc",
    "docs": "babel-node ./doc/doc.js",
    "server": "node ./test/serverBoot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyranid-org/tyranid"
  },
  "engine": {
    "node": ">=4.3.0"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "4.0.36",
    "@types/mongodb": "2.2.7",
    "babel-core": "6.14.0",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-preset-stage-0": "6.5.0",
    "chalk": "1.1.1",
    "continuation-local-storage": "3.1.6",
    "faker": "3.1.0",
    "glob": "7.0.5",
    "hooker": "0.2.3",
    "lodash": "3.9.3",
    "moment": "2.11.2",
    "mongodb": "2.1.4",
    "on-finished": "2.3.0",
    "uglify-js": "2.8.16"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "body-parser": "1.17.2",
    "chai": "3.0.0",
    "chai-as-promised": "5.1.0",
    "codecov": "1.0.1",
    "eslint": "3.9.0",
    "express": "4.15.2",
    "jquery": "3.1.1",
    "jsdom": "9.8.3",
    "mocha": "2.2.5",
    "node-fetch": "1.7.1",
    "nyc": "8.3.0",
    "typescript": "2.4.1",
    "yargs": "3.12.0"
  },
  "babel": {
    "sourceMaps": "both",
    "compact": false,
    "presets": [
      "stage-0",
      "es2015-node4"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
