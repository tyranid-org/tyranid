{
  "name": "tyranid",
  "version": "0.5.65",
  "description": "Metadata Library to faciliate working generically with data (MongoDB, ODM)",
  "main": "dist/src/tyranid.js",
  "typings": "tyranid.d.ts",
  "scripts": {
    "docs": "ts-node ./scripts/generate-docs.js",
    "test": "npm run mocha",
    "build": "npm run tsc",
    "test-travis": "npm run pretest && npm run mocha-coverage",
    "generate-typings": "ts-node ./scripts/generate-typings.js",
    "pretest": "npm run tsc",
    "prepublish": "npm run test",
    "watch": "rm -rf ./dist/ && tsc -w",
    "tsc:tests": "npm run generate-typings && tsc -p ./test",
    "tsc": "rm -rf ./dist/ && tsc",
    "mocha": "npm run tsc:tests && mocha \"./dist/test/test.js\" --read false -t 100000",
    "mocha-coverage": "nyc mocha \"./dist/test/test.js\" --read false",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "server": "node ./test/serverBoot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyranid-org/tyranid"
  },
  "engine": {
    "node": ">=8.1.4"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^2.1.0",
    "continuation-local-storage": "3.1.6",
    "faker": "^4.1.0",
    "glob": "7.0.5",
    "json-stable-stringify": "1.0.1",
    "lodash": "^3.10.1",
    "moment": "2.20.1",
    "node-fetch": "1.7.1",
    "on-finished": "2.3.0",
    "socket.io": "2.0.3",
    "ts-node": "^3.2.0",
    "uglify-js": "2.8.16",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "mongodb": "3.1.10",
    "typescript": "^2.5.3"
  }
}
